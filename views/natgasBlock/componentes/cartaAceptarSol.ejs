<% for(let NGBActual of ngbsPorAceptar) { %>
<div class="row">
    <div class="col s12 m3 l13"></div>
    <div class="col s12 m6 l13">
        <div class="card" style="border-radius: 15px;">
            <div class="card-content" style="padding: 10px;">
                <div class="row">
                    <div class="col s12 m7 l13">
                        <span class="txt">
                            <h5 style="margin-left: 5%;">
                                <span style="font-weight: 600;">Fecha de solicitud:</span> <%=NGBActual.fechaSolicitud.toString().substr(4,11) %>
                            </h5>
                        </span>
                    </div>
                    <div class="col s12 m2 l13"></div>
                </div>

                <div class="row">
                    <div class="col s12 m7 l13">
                        <h6 style="margin-left: 5%;"><span style="font-weight: 600;">Fecha de uso:</span> <%=NGBActual.fechaUsoNGB.toString().substr(4,11)%></h6>
                    </div>
                    <div class="col s12 m2 l13"></div>
                    <div class="col s12 m1 l13">
                        <a class="btn-large green"><span style="font-weight: 600;">Aceptar</span></a>
                    </div>
                </div>
                <div class="row">
                    <h6 style="margin-left: 4%;">
                        <span style="font-weight: 600;">Empleado que solicit√≥:</span><%=NGBActual.nombre + " " + NGBActual.apellidoPaterno + " " +NGBActual.apellidoMaterno %>
                    </h6>
                </div>
            </div>
        </div>
    </div>
</div>
<% } %>