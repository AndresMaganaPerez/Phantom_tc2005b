<% for (let solicitud of solicitudes) { %>
    <div class="row">
        <div class="col s12 m2 l13"></div>
        <div class="col s12 m8 l13">
            <div class="card" style="border-radius: 15px;">
                <div class="card-content" style="padding: 10px;">
                    <div class="row">
                        <div class="col s12 m7 l13">
                            <span class="txt">
                                <h5 style="margin-left: 5%;">
                                    <span style="font-weight: 600;">Fecha de solicitud:</span>
                                    <%= solicitud.fechaSolicitud.toString().substring(4,15) %>
                                </h5>
                            </span>
                        </div>
                        <div class="col s12 m2 l13"></div>
                    </div>
                    <div class="row">
                        <div class="col s12 m5 l13">
                            <h6 style="margin-left: 7%;"><span style="font-weight: 600;">Fecha de uso:</span>
                                <%= solicitud.fechaUsoNGB.toString().substring(4,15) %> 
                                
                            </h6>
                        </div>
                        <div>
                            <div class="col s12 m2 l13"></div>
                            <%- include('bloques/bloqueAceptar_Persona.ejs') %>
                            <div class="col s12 m1 l13"></div>
                            <div class="col s12 m2 l13" style="margin-left: 0.5%;">Por <%= solicitud.Lider + " " + solicitud.lApellidoPaterno + " " + solicitud.lApellidoMaterno %> </div>
                        </div>
                    </div>
                    <div class="row">
                        <h6 style="margin-left: 4%;">
                            <span style="font-weight: 600;">Empleado que solicit√≥:</span>
                            <%= solicitud.Quien_Solicita + " " + solicitud.qsApellidoPaterno + " " + solicitud.qsApellidoMaterno%>
                        </h6>
                    </div>
                </div>
                <div class="card-image waves-effect waves-block waves-light">

                </div>
            </div>
        </div>
    </div>
<% } %>
