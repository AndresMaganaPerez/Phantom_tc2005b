<% for (let solicitud of solicitudes) { %>
    <div class="row">
        <div class="col s12 m2 l13"></div>
        <div class="col s12 m8 l13">
            <div class="card" style="border-radius: 15px;">
                <div class="card-content" style="padding: 10px;">
                    <div class="row">
                        <div class="col s12 m8 l13">
                            <span class="txt">
                                <h5 style="margin-left: 4%;">
                                    <span style="font-weight: 600;">Empleado que solicit√≥:</span>
                                    <%= solicitud.Quien_Solicita + " " + solicitud.qsApellidoPaterno + " " + solicitud.qsApellidoMaterno%>
                                </h5>  
                            </span>
                        </div>
                        <div class="col s12 m1 l13"></div>
                    </div>
                    <div class="row">
                        <div class="col s12 m7 l13">
                            <span class="text">
                                <h6 style="margin-left: 4%;"><span style="font-weight: 600;">Fecha de uso:</span>
                                    <%= solicitud.fechaUsoNGB.getDate() + '/' + (solicitud.fechaUsoNGB.getMonth() + 1) + '/' + solicitud.fechaUsoNGB.getFullYear() %> 
                                </h6>
                            </span>
                        </div>
                        <div>
                            <div class="col s12 m1 l13"></div>
                            <div class="col s12 m2 l13">
                                <%- include('../bloques/bloqueAceptar_Persona.ejs') %>
                            </div>
                            <div class="col s12 m2 l13">
                                <div style="margin-left: 0.5%;">Por <%= solicitud.Lider + " " + solicitud.lApellidoPaterno + " " + solicitud.lApellidoMaterno %> </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col s12 m7 l13">
                            <span class="text">
                                <h6 style="margin-left: 4%;">
                                    <span style="font-weight: 600;">Fecha de solicitud:</span>
                                    <%= solicitud.fechaSolicitud.getDate() + '/' + (solicitud.fechaSolicitud.getMonth() + 1) + '/' + solicitud.fechaSolicitud.getFullYear() %>
                                </h6>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="card-image waves-effect waves-block waves-light">

                </div>
            </div>
        </div>
    </div>
<% } %>
