<% for (let ngb of ngbsPorAceptar) { %>  
    <div class="row">
        <div class="col s12 m3 l13"></div>
        <div class="col s12 m6 l13">
            <div class="card" style="border-radius: 15px;">
                <div class="card-content" style="padding: 10px;">
                    <div class="row">
                        <div class="col s12 m7 l13">
                            <span class="txt">
                                <h5 style="margin-left: 5%;">
                                    <span style="font-weight: 600;">Fecha de solicitud:</span>
                                    <%= ngb.getDay() + '/' + ngb.getMonth() + '/' + ngb.getFullYear() %>
                                </h5>
                            </span>
                        </div>
                        <div class="col s12 m2 l13"></div>
                    </div>

                    <div class="row">
                        <div class="col s12 m7 l13">
                            <h6 style="margin-left: 5%;"><span style="font-weight: 600;">Fecha de uso:</span>
                                <%= ngb.fechaUsoNGB.getDay() + ngb.fechaUsoNGB.getMonth() + ngb.fechaUsoNGB.getFullYear() %>
                            </h6>
                        </div>
                        <div class="col s12 m2 l13"></div>
                        <div class="col s12 m1 l13">
                            <form action="/natgas_blocks/click_aceptar_ngb" method="POST">
                                <input type="text" value="<%= ngb.idNatgasBlocks %>" name="idngb" hidden>
                                <input type="text" value="<%= ngb.idqs %>" name="idqs" hidden>
                                <input type="submit" class="btn waves-light green" style="color: white;" value="Aceptar" name="aceptado">
                            </form>
                            
                        </div>
                    </div>
                    <div class="row">
                        <h6 style="margin-left: 4%;">
                            <span style="font-weight: 600;">Empleado que solicit√≥:</span>
                            <%= ngb.Quien_solicita + " " + ngb.qsApellidoPaterno + " " + ngb.qsApellidoMaterno %>
                        </h6>
                    </div>
                </div>
            </div>
        </div>
    </div>
<% } %>