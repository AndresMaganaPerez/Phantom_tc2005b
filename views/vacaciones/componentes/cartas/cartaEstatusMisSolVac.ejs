    <div class="row">
        <div class="col s12 m2 l13"></div>
        <div class="col s12 m8 l13">
            <div class="card " style="border-radius: 15px;">
                <div class="card-content" style="padding: 10px;">
                    <div class="row">
                        <div class="col s12 m12 l13">
                            <h5 style="margin-left: 1.8vw;">
                                <span style="font-weight: 600;">
                                    Número de Nómina:   
                                </span>
                                <%= solicitud.idEmpleado %>
                            </h5>
                        </div>
                        <div class="col s12 m4 l13">
                            <h6>
                                <span style="font-weight: 600; margin-left: 1.8vw;">Suplente:</span> <%= solicitud.suplente %>
                            </h6>
                        </div>
                        <% if ((solicitud.solicitudAceptadaEstatus == 1 || solicitud.solicitudAceptadaEstatus == null) && solicitud.fechaInicio > diaActual) { %>
                            <div class="col s12 m4 l13"></div>
                            <div class="col s12 m2 l13" style="margin-left: 1.4vw;">
                                <%- include('../botones/botonCancelar.ejs') %>
                            </div>
                        <% } %>
                    </div>
                    <div class="row">
                        <div class="col s12 m7 l13">
                            <h6 style="margin-left: 1.8vw;">
                                <span style="font-weight: 600;">Fecha Inicio:</span>
                                <%= solicitud.fechaInicio.getDate() + '/' + (solicitud.fechaInicio.getMonth() + 1) + '/' + solicitud.fechaInicio.getFullYear() %>
                                |
                                <span style="font-weight: 600;">Fecha Fin:</span>
                                <%= solicitud.fechaFin.getDate() + '/' + (solicitud.fechaFin.getMonth() + 1) + '/' + solicitud.fechaFin.getFullYear()%>
                            </h6>
                        </div>
                        <div class="col s12 m2 l13">
                            <% if (solicitud.solicitudAceptadaEstatus == null) { %>
                                <%- include('../bloques/componentesBloque/cartas/cartaSinStatus.ejs') %>
                            <% } else if (solicitud.solicitudAceptadaEstatus == 1) { %>
                                <%- include('../bloques/componentesBloque/cartas/cartaAceptado.ejs') %>
                            <% } else if (solicitud.solicitudAceptadaEstatus == 0) { %>
                                <%- include ('../bloques/componentesBloque/cartas/cartaRechazado', {nota: solicitud.Nota}) %>
                            <% } %>
                        </div>
                        <div class="col s12 m2 l13">
                            <% if (solicitud.solicitudAceptadaEstatus == 1 ) { %>
                                Por <%= lider.nombre + ' ' + lider.apellidoPaterno + ' ' + lider.apellidoMaterno %>
                            <% } else if (solicitud.solicitudAceptadaEstatus == 0) { %>
                                Por <%= lider.nombre + ' ' + lider.apellidoPaterno + ' ' + lider.apellidoMaterno %>
                            <% } %>
                        </div> 
                        
                    </div>
                    <div class="row">
                        <div class="col s12 m6 l13">
                            <h6 style="margin-left: 1.8vw;">
                                <span style="font-weight: 600;">Fecha Reanudación: </span> <%=
                                solicitud.fechaReanudacion.getDate() + '/' + (solicitud.fechaReanudacion.getMonth() + 1) + '/' + solicitud.fechaReanudacion.getFullYear() %>
                            </h6>
                        </div>
                        <div class="col s12 m2 l13"></div>
                        <div class="col s12 m3 l13 center" style="margin-left: 1.4vw;">
                        </div>
                    </div>
                    <div class="row">
                        <h6 style="margin-left: 2.4vw;">
                            <span class='txt'>
                                    <span style="font-weight: 600;">Fecha de solicitud: </span> <%= solicitud.fechaSolicitud.getDate() + '/' + 
                                    (solicitud.fechaSolicitud.getMonth() + 1) + '/' + solicitud.fechaSolicitud.getFullYear() %>
                            </span>
                        </h6>
                    </div>
                </div>
                <div class="card-image waves-effect waves-block waves-light"></div>
            </div>
        </div>
    </div>